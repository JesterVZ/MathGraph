<Window x:Class="MathGraph.SettingsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MathGraph"
        mc:Ignorable="d"
        Title="SettingsWindow" Height="450" Width="450">
    <Grid>
        <Grid VerticalAlignment="Top" Height="30" Background="#FF00B9FF" MouseDown="Grid_MouseDown">
            <TextBlock HorizontalAlignment="Center" VerticalAlignment="Center" Text="Settings" FontSize="16" FontWeight="Bold" Foreground="White"/>
        </Grid>
    </Grid>
</Window>
